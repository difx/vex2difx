enddef;
*------------------------------------------------------------------------------
$STATION;
*
def Kp;
     ref $SITE = VLBA_KP;
     ref $ANTENNA = VLBA_KP;
     ref $DAS = 2VLBA+NONE<;
enddef;
*------------------------------------------------------------------------------
$PROCEDURES;
*
*The following def is used with FORMAT=NONE.
*
def Mode_01;
     procedure_name_prefix =  "01";
     tape_change =  420 sec;
     headstack_motion =    6 sec;
     new_source_command =    5 sec;
     new_tape_setup =   20 sec;
